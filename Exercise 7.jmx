          <stringProp name="filename"></stringProp>
          <stringProp name="script">import java.text.*;
import java.io.*;
import java.util.*;

ArrayList allLines = new ArrayList();     

try {
    File file = new File(vars.get(&quot;file_path&quot;));

    if (!file.exists()) {
        throw new Exception (&quot;ERROR: file &quot; + csvTest + &quot; not found in &quot; + csvDir + &quot; directory.&quot;);
    }

	//Get number of lines in the file
	BufferedReader bufRdr = new BufferedReader(new FileReader(file));
	String line = null;

    while((line = bufRdr.readLine()) != null) {
        allLines.add(line);
    }
    bufRdr.close();   

    vars.put(&quot;num_of_lines_in_file&quot;,Integer.toString(allLines.size())); 
   //Create a set of all urls 
    for (i = 0; i&lt;allLines.size();i++){
    	vars.put(&quot;url_&quot;+Integer.toString(i+1),allLines.get(i));  
 //   	log.info(allLines.get(i));
     }
}

catch (Exception ex) {
    IsSuccess = false; 
    log.error(ex.getMessage());
    System.err.println(ex.getMessage());
}
catch (Throwable thex) {
    System.err.println(thex.getMessage());
}
</stringProp>
